{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "module": "ESNext",
    "jsx": "preserve",
    "noEmit": false
